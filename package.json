{
  "name": "sidegit",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "gen-env": "gen-env-types server/.env -o server/src/env.d.ts -e .",
    "build": "tsc -p server/tsconfig.json",
    "watch": "tsc -w -p server/tsconfig.json",
    "dev": "nodemon server/dist/index.js",
    "start": "node server/dist/index.js",
    "start2": "ts-node server/src/index.ts",
    "dev2": "nodemon --exec ts-node server/src/index.ts",
    "mikro-orm": "mikro-orm --config server/src/mikro-orm.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/node": "^22.13.17",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@mikro-orm/cli": "^6.4.11",
    "@mikro-orm/core": "^6.4.11",
    "@mikro-orm/migrations": "^6.4.11",
    "@mikro-orm/postgresql": "^6.4.11",
    "dotenv": "^16.4.7",
    "pg": "^8.14.1"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      ".src/mikro-orm.configs.ts",
      ".dist/mikro-orm.configs.js"
    ]
  }
}
